{
    "jsonContextGenerator": {
        "ignoredTypes": [
            "System.Threading.Tasks.Task",
            "System.IO.Stream"
        ]
    },
    "proxiesGenerator": {
        "outputFile": "api-proxies-angular.ts",
        "swaggerUrl": "https://localhost:7020/swagger/v1/swagger.json",
        "typeScriptClient": {
            "className": "{controller}Client",
            "generateDtoTypes": true,
            "generateClientInterfaces": true,
            "generateClientClasses": true,
            "generateOptionalParameters": false,
            "excludedParameterNames": [],
            "wrapResponses": false,
            "wrapResponseMethods": [],
            "generateResponseClasses": true,
            "responseClass": "SwaggerResponse",
            "template": "Angular",
            "promiseType": "Promise",
            "wrapDtoExceptions": false,
            "clientBaseClass": "BaseClient",
            "configurationClass": null,
            "useTransformOptionsMethod": false,
            "useTransformResultMethod": false,
            "baseUrlTokenName": "API_BASE_URL",
            "protectedMethods": [],
            "importRequiredTypes": true,
            "useGetBaseUrlMethod": true,
            "queryNullValue": "",
            "exceptionClass": "ApiError",
            "useAbortSignal": false,
            "httpClass": "HttpClient",
            "withCredentials": false,
            "rxJsVersion": 7.0,
            "useSingletonProvider": false,
            "injectionTokenType": "InjectionToken",
            "includeHttpContext": false,
            "typeScriptGeneratorSettings": {
                "typeScriptVersion": 4.3,
                "markOptionalProperties": false,
                "typeStyle": "Interface",
                "dateTimeType": "string",
                "enumStyle": "Enum",
                "useLeafType": false,
                "moduleName": "",
                "namespace": "",
                "extendedClasses": [],
                "extensionCode": "base-client.ts",
                "classTypes": [],
                "nullValue": "Undefined",
                "handleReferences": false,
                "generateCloneMethod": false,
                "generateConstructorInterface": false,
                "convertConstructorInterfaceData": false,
                "exportTypes": true,
                "inlineNamedDictionaries": false,
                "generateTypeCheckFunctions": false,
                "generateDefaultValues": false,
                "excludedTypeNames": [],
                "templateDirectory": null,
                "inlineNamedAny": false
            }
        }
    }
}